{
  "name": "<%= slugname %>",
  "version": "0.0.0",
  "main": "src/<%= slugname %>.coffee",
  "description": "<%= props.description %>",
  "homepage": "<%= props.homepage %>",
  "bugs": "<%= repoUrl %>/issues",
  "author": {
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
    "url": "<%= props.authorUrl %>"<% } %>
  },
  "repository": {
    "type": "git",
    "url": "<%= repoUrl %>"
  },
  "licenses": [
    {
      "type": "<%= props.license %>"
    }
  ],
  "dependencies": {
    "bootstrap": "~3.3.4",
    "jquery": "~2.1.3"
  }
}
